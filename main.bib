	file = {Barkan et al_2024_Identification of Potent, Broad-Spectrum Coronavirus Main Protease Inhibitors.pdf:/Users/alexpayne/Scientific_Packages/Zotero/storage/I5R9H4N3/Barkan et al_2024_Identification of Potent, Broad-Spectrum Coronavirus Main Protease Inhibitors.pdf:application/pdf},
