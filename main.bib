@article{dampalla_structure-guided_2022,
	title = {Structure-{Guided} {Design} of {Potent} {Spirocyclic} {Inhibitors} of {Severe} {Acute} {Respiratory} {Syndrome} {Coronavirus}-2 {3C}-like {Protease}},
